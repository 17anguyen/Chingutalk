
<!-- right column -->
<div class="col g-6">
    <h2>{{searchTerm}}</h2>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">First</th>
                <th scope="col">Last</th>
            </tr>
        </thead>
        <tbody>
            {{#each users}}
            <tr>
                <td>{{id}}</td>
                <td>{{firstName}}</td>
                <td>{{lastName}}</td>
            </tr>

            {{/each}}
        </tbody>
    </table>
</div>

<script src="/js/search.js"></script>
<script src="https://upload-widget.cloudinary.com/global/all.js" type="text/javascript"></script>
