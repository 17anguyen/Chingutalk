<div class="row">
    <!-- left column -->


    <!-- right column -->
    <div class="card m-6" style="background-color:#BEB4AA; height:100%; align-items:center;">

        <figure class="ratio-4x4">
            <img class="ratio-4x4 profile-pic" src="{{userData.photoUrl}}" alt="profile picture">
        </figure>
        {{!-- <div class="card-header">Log In</div> --}}
        <h2>{{userData.firstName}}'s page</h2>
        <h3>Interests</h3>
        {{!-- {{#each userData}} --}}
        <ul>
            {{#each userData.interests}}
            <li>{{interest}}</li>
            {{/each}}
            {{!-- {{/each}} --}}
        </ul>
        <p>{{userData.biography}}</p>
        <div>


            <a href="/chat" class="btn btn-outline-light" role="button" data-bs-toggle="button" id="msgBtn">

                Start Chatting
                <img src="/Assets/chat bubble icon.png" style="height: 40px; width:auto">

            </a>

            {{!-- <button type="button" class="btn btn-outline-light" id="msgBtn">
                Start Chatting

                <a href="/chat"></a>
            </button> --}}
            <div id="messages"></div>
        </div>
        <hr>
        {{!-- {{/each}} --}}
    </div>





</div>


